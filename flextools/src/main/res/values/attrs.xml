<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlexFare">
        <attr name="diagonal_angle" format="integer" />
        <attr name="diagonal_handleMargins" format="boolean"/>
        <attr name="diagonal_position" format="enum">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="4" />
            <enum name="top" value="8"/>
        </attr>
        <attr name="diagonal_direction" format="enum">
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CurvedImage">
        <attr name="arc_height" format="dimension" />
        <attr name="arc_position">
            <enum name="bottom" value="0"/>
            <enum name="top" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="3"/>
        </attr>
        <attr name="arc_cropDirection" format="enum">
            <enum name="cropInside" value="0" />
            <enum name="cropOutside" value="1"/>
        </attr>
    </declare-styleable>
</resources>